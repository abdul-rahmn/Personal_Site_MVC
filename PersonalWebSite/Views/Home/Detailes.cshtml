@{
    DetailesC detailesC = ViewBag.DT;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css">
    <title>Detailes</title>
</head>

<body>
    <header>
        <a href="/" class="logo">AbdulRahman</a>
    </header>
    <section class="cards" id="detailes">
        <h2 class="title">@detailesC.title</h2>
        <p>@detailesC.description</p>
        <br>
        <p><span id="detailes-techno">Technologies used:</span></p>
        <br>
        <p>- programming language: <span class="detailes-project">@detailesC.lang</span></p>
        <p>- FrameWork: <span class="detailes-project">@detailesC.frameWork</span></p>
        <p>- DataBase: <span class="detailes-project">@detailesC.dataBase</span></p>
        <p>- API: <span class="detailes-project">@detailesC.api</span></p>
        <br><hr><br>
    </section>
    <section class="detailes-cards" id="detailes">
        <h3 class="title">| Some Images |</h3>
        <div class="detailes-content">
            @foreach (var item in detailesC.images)
            {
                <div class="detailes-card">
                    <div class="detailes-image">
                        <img src="~/images/@item" alt="">
                    </div>
                </div>
            }
        </div>
    </section>
    <footer class="footer">
        <p class="footer-title">| <span>AbdulRahman AL-Ismail</span> |</p>
        <div class="social-icons">
            <a href="https://www.linkedin.com/in/abdul-rahman-al-ismail-48178323a?lipi=urn%3Ali%3Apage%3Ad_flagship3_profile_view_base_contact_details%3BPpUobvFYQHefG94EOBuu9A%3D%3D"
               target="_blank"><i class="fa-brands fa-linkedin"></i></a>
            <a href="https://github.com/abdul-rahmn" target="_blank"><i class="fa-brands fa-github"></i></a>
            <a href="https://www.facebook.com/profile.php?id=100048513832291" target="_blank"><i class="fa-brands fa-facebook"></i></a>
            <a href="https://wa.me/905518061064" target="_blank"><i class="fa-brands fa-whatsapp"></i></a>
            <a href="https://t.me/abdul_rahman771" target="_blank"><i class="fa-brands fa-telegram"></i></a>
            <a href="mailto:alasmaylbdalrhmn4@gmail.com" target="_blank"><i class="fa-solid fa-envelope"></i></a>
        </div>
    </footer>
</body>

</html>